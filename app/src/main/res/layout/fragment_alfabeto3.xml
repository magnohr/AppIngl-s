    <ImageView
        android:id="@+id/kk"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/k"
        app:layout_constraintBottom_toTopOf="@+id/nn"
        app:layout_constraintEnd_toStartOf="@+id/ll"
        app:layout_constraintStart_toEndOf="@+id/jj"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/ll"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:src="@drawable/l"
        app:layout_constraintBottom_toTopOf="@+id/oo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/kk"
        app:layout_constraintTop_toTopOf="parent" />

