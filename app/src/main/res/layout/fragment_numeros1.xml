<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ui.Numeros.Numeros1Fragment">

    <ImageView
        android:id="@+id/um"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="28dp"
        android:src="@drawable/a1"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="@+id/textView13"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/dois"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="22dp"
        android:src="@drawable/a2"
        app:layout_constraintBottom_toTopOf="@+id/textView16"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/quatro"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/tres"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="11dp"
        android:src="@drawable/a3"
        app:layout_constraintBottom_toTopOf="@+id/textView17"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="@+id/textView15" />

    <ImageView
        android:id="@+id/quatro"
        android:layout_width="0dp"
        android:layout_height="100dp"
        android:layout_marginTop="165dp"
        android:layout_marginBottom="165dp"
        android:src="@drawable/a4"
        app:layout_constraintBottom_toTopOf="@+id/textView21"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/seis"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/cinco"
        android:layout_width="150dp"
        android:layout_height="98dp"
        android:layout_marginTop="33dp"
        android:layout_marginBottom="34dp"
        android:src="@drawable/a5"
        app:layout_constraintBottom_toTopOf="@+id/textView20"
        app:layout_constraintEnd_toEndOf="@+id/textView17"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/quatro" />

    <ImageView
        android:id="@+id/seis"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="6dp"
        android:src="@drawable/a6"
        app:layout_constraintBottom_toBottomOf="@+id/cinco"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/oito"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <ImageView
        android:id="@+id/oito"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/a8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <ImageView
        android:id="@+id/nove"
        android:layout_width="101dp"
        android:layout_height="100dp"
        android:src="@drawable/a9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <ImageView
        android:id="@+id/dez"
        android:layout_width="150dp"
        android:layout_height="99dp"
        android:src="@drawable/adez"
        app:layout_constraintBottom_toTopOf="@+id/textView19"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:text="One"
        app:layout_constraintBottom_toTopOf="@+id/um"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:text="Two"
        app:layout_constraintBottom_toTopOf="@+id/dois"
        app:layout_constraintEnd_toEndOf="@+id/dois"
        app:layout_constraintStart_toStartOf="@+id/dois" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="147dp"
        android:text="Three"
        app:layout_constraintEnd_toEndOf="@+id/tres"
        app:layout_constraintStart_toStartOf="@+id/tres"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:text="Four"
        app:layout_constraintBottom_toTopOf="@+id/quatro"
        app:layout_constraintEnd_toEndOf="@+id/quatro" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="49dp"
        android:text="Five"
        app:layout_constraintBottom_toTopOf="@+id/cinco"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Six"
        app:layout_constraintEnd_toEndOf="@+id/seis"
        app:layout_constraintStart_toStartOf="@+id/seis"
        app:layout_constraintTop_toBottomOf="@+id/quatro" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ten"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:text="Seven"
        app:layout_constraintBottom_toTopOf="@+id/oito"
        app:layout_constraintStart_toStartOf="@+id/sete"
        app:layout_constraintTop_toTopOf="@+id/textView31" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:text="Nine"
        app:layout_constraintBottom_toTopOf="@+id/oito"
        app:layout_constraintStart_toStartOf="@+id/nove"
        app:layout_constraintTop_toTopOf="@+id/textView31" />

    <ImageView
        android:id="@+id/sete"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:src="@drawable/a7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="34dp"
        android:layout_marginTop="264dp"
        android:layout_marginBottom="263dp"
        android:text="Eight"
        app:layout_constraintBottom_toBottomOf="@+id/dez"
        app:layout_constraintStart_toStartOf="@+id/oito"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />
</androidx.constraintlayout.widget.ConstraintLayout>